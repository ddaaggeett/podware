{
  "name": "podware",
  "license": "GPL-3.0",
  "description": "podcast software",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pull": "babel-node src/devices/adb/pullScript",
    "rethinkdb": "rethinkdb",
    "server-script": "nodemon --exec babel-node ./src/server",
    "server": "npm-run-all --parallel rethinkdb server-script",
    "bundle": "cd android && ./gradlew assembleRelease",
    "bundle-deploy": "adb uninstall com.podware_camera && react-native run-android --variant=release",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.16.4",
    "merge": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "react": "16.6.3",
    "react-native": "0.57.6",
    "react-native-camera": "^1.4.3",
    "react-native-device-info": "^0.24.3",
    "react-native-keep-awake": "^4.0.0",
    "react-navigation": "^3.0.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "rethinkdb": "^2.3.3",
    "rn-fetch-blob": "^0.10.13",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "usb": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "nodemon": "^1.18.6",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
